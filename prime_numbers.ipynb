   "source": [
    "n = int(input(\"Enter a positive number: \"))\n",
    "\n",
    "liste = []\n",
    "\n",
    "for num in range(1, n + 1):  \n",
    "   if num > 1:  \n",
    "       for i in range(2,num):  \n",
    "           if (num % i) == 0:  \n",
    "               break  \n",
    "       else:  \n",
    "           liste.append(num)\n",
    "\n",
    "print(liste)"
   ]
